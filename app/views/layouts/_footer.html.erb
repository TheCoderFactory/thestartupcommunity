<% cache('footer') do %>
<footer>
  
  
      <div class="container">

        <div class="navbar navbar-fixed-bottom">
          <div class="navbar-inner">
           
            
            
              <ul class="nav">
                <li><a>&copy; 2013 The Startup Community</a></li>
                <li><a href="http://www.sydneydevcamp.com" target"_blank">Powered by <%= image_tag('FB-logo.png', :class => "footer-logo") %> <span style="color:yellow">Sydney Dev Camp</span></a></li>
                <li><%= link_to 'Privacy', privacy_path %></li>
                <li><%= link_to 'Terms', terms_path %></li>
                <li><%= link_to 'Contact', new_contact_form_path, :title => "Report abuse/problems, suggest a feature" %></li>
                <% if @about_page %>
                  <li><%= link_to 'About', about_path(@about_page), :title => "About The Startup Community" %></li>
                <% end %>
                <% if user_signed_in? %>
                  <% if current_user.has_role? :admin %>
                    <li><%= link_to 'Admin', admin_path %></li>
                  <% end %>
                <% end %>
              </ul>
              
            
          </div><!-- /.navbar-inner -->
        </div><!-- /.navbar -->

      </div> <!-- /.container -->
</footer>
<% end %>