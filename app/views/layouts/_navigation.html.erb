    <!-- NAVBAR
    ================================================== -->
    <div class="navbar-wrapper">
      <!-- Wrap the .navbar in .container to center it within the absolutely positioned parent. -->
      <div class="container">

        <div class="navbar">
          <div class="navbar-inner">
            
            
            <%= link_to "The Startup Community - Sydney", root_path, :class => 'brand' %>
            
              <ul class="nav">
                <li><%= link_to 'Profiles', user_profiles_path %></li>
                <li><%= link_to 'Startups', business_profiles_path %></li>
                <li><%= link_to 'Sydney Startup Map', startup_maps_path %></li>
                </ul>
                <ul class="nav pull-right">
                <% if user_signed_in? %>
                  <li class="dropdown"><a href="#" class="dropdown-toggle" data-toggle="dropdown">Account <b class="caret"></b></a>
                    <ul class="dropdown-menu">
                  <% if user_signed_in? %>
                    <li>
                    <%= link_to 'My account', user_path(current_user) %>
                    </li>
                    <% if current_user.has_role? :admin %>
                      <li>
                      <%= link_to 'Admin', admin_path %>
                      </li>
                    <% end %>
                  <li>
                    <%= link_to 'Logout', destroy_user_session_path, :method=>'delete' %>
                  </li>
                  <% end %>
                </ul>
              <% else %>
                
                 <li><%= link_to 'Login/Sign up', new_user_session_path %></li>
                </ul>
              <% end %>
            
              
            
          </div><!-- /.navbar-inner -->
        </div><!-- /.navbar -->

      </div> <!-- /.container -->
    </div><!-- /.navbar-wrapper -->
